prompt
prompt Creating table TARI
prompt ======================
prompt

CREATE TABLE TARI
(ID_TARA CHAR(2) CONSTRAINT P_key_tari PRIMARY KEY,
 DENUMIRE_TARA VARCHAR2(40)
);
ALTER TABLE TARI
  ADD CONSTRAINT ID_TARA_NN CHECK ("ID_TARA" IS NOT NULL);

prompt
prompt Creating table CLIENTI
prompt ======================
prompt

CREATE TABLE CLIENTI 
(ID_CLIENT NUMBER(6) NOT NULL,
NUME VARCHAR2(30),
PRENUME VARCHAR2(30),
TELEFON VARCHAR2(20),
EMAIL VARCHAR2(30),
SALARIU NUMBER(8),
DATA_NASTERE DATE,
SEX VARCHAR2(1),
ID_TARA CHAR(2)
);
ALTER TABLE CLIENTI
  ADD CONSTRAINT P_KEY_CL PRIMARY KEY (ID_CLIENT);
ALTER TABLE CLIENTI
  ADD CONSTRAINT CL_NUME_NN CHECK ("NUME" IS NOT NULL);
ALTER TABLE CLIENTI
  ADD CONSTRAINT CL_PRENUME_NN CHECK ("PRENUME" IS NOT NULL);
ALTER TABLE CLIENTI
  ADD CONSTRAINT CLIENTI_DATA_NN CHECK ("DATA_NASTERE" IS NOT NULL);
ALTER TABLE CLIENTI
  ADD CONSTRAINT TARA_CL_FK FOREIGN KEY (ID_TARA) REFERENCES TARI(ID_TARA);

prompt
prompt Creating table SUCURSALE
prompt ======================
prompt

CREATE TABLE SUCURSALE(
ID_SUCURSALA NUMBER(6) NOT NULL,
 NUME VARCHAR2(50),
 COD_POSTAL VARCHAR2(12),
 ID_TARA CHAR(2)
);
ALTER TABLE SUCURSALE
  ADD CONSTRAINT S_NUME_NN CHECK ("NUME" IS NOT NULL);
ALTER TABLE SUCURSALE
  ADD CONSTRAINT P_KEY_S PRIMARY KEY (ID_SUCURSALA);
ALTER TABLE SUCURSALE
  ADD CONSTRAINT SUC_T_FK FOREIGN KEY (ID_TARA) REFERENCES TARI (ID_TARA);


prompt
prompt Creating table ANGAJATI
prompt ======================
prompt

CREATE TABLE ANGAJATI(
ID_ANGAJAT NUMBER(6) NOT NULL,
NUME VARCHAR2(50),
PRENUME VARCHAR2(50), 
DATA_ANGAJARE DATE,
SALARIU NUMBER(10, 2),
ID_MANAGER NUMBER(6),
ID_SUCURSALA NUMBER(6)
);
ALTER TABLE ANGAJATI
  ADD CONSTRAINT P_KEY_A PRIMARY KEY (ID_ANGAJAT);
ALTER TABLE ANGAJATI
  ADD CONSTRAINT ANGJ_S_FK FOREIGN KEY (ID_SUCURSALA) REFERENCES SUCURSALE (ID_SUCURSALA);
ALTER TABLE ANGAJATI
  ADD CONSTRAINT ANG_DATA_ANG_NN CHECK ("DATA_ANGAJARE" IS NOT NULL);
ALTER TABLE ANGAJATI
  ADD CONSTRAINT ANG_NUME_NN CHECK ("NUME" IS NOT NULL);
ALTER TABLE ANGAJATI
  ADD CONSTRAINT ANG_PRENUME_NN CHECK ("PRENUME" IS NOT NULL);
ALTER TABLE ANGAJATI
  ADD CONSTRAINT ANG_SALARIUL_MIN CHECK (SALARIU > 0);

prompt
prompt Creating table CONTURI
prompt ======================
prompt

CREATE TABLE CONTURI(
ID_CONT NUMBER(6) NOT NULL,
ID_CLIENT NUMBER(6), 
SOLD NUMBER(10, 2),
TIP_CONT VARCHAR2(20)
);
ALTER TABLE CONTURI
  ADD CONSTRAINT P_KEY_CO PRIMARY KEY (ID_CONT);
ALTER TABLE CONTURI
  ADD CONSTRAINT CONT_CL_FK FOREIGN KEY (ID_CLIENT) REFERENCES CLIENTI(ID_CLIENT) ON DELETE CASCADE;
ALTER TABLE CONTURI 
  ADD CONSTRAINT TIP_C_NN CHECK ("TIP_CONT" IS NOT NULL);


prompt
prompt Creating table TRANZACTII
prompt ======================
prompt

CREATE TABLE TRANZACTII(
ID_TRANZACTIE NUMBER(10) NOT NULL,
ID_CONT NUMBER(6),
SUMA NUMBER(12, 2), 
TIP_TRANZACTIE VARCHAR2(20),
DATA_TRANZACTIE DATE,
ID_ANGAJAT NUMBER(6)
);
ALTER TABLE TRANZACTII
  ADD CONSTRAINT P_KEY_T PRIMARY KEY (ID_TRANZACTIE);
ALTER TABLE TRANZACTII
  ADD CONSTRAINT CONT_T_FK FOREIGN KEY (ID_CONT) REFERENCES CONTURI(ID_CONT);
ALTER TABLE TRANZACTII
  ADD CONSTRAINT TRANZ_MIN CHECK (SUMA > 5);
ALTER TABLE TRANZACTII
  ADD CONSTRAINT DATA_NN CHECK("DATA_TRANZACTIE" IS NOT NULL);
ALTER TABLE TRANZACTII
  ADD CONSTRAINT TRANZ_A_FK FOREIGN KEY (ID_ANGAJAT) REFERENCES ANGAJATI(ID_ANGAJAT);

prompt
prompt Creating table CREDITE
prompt ======================
prompt

CREATE TABLE CREDITE(
ID_CREDIT NUMBER(8) NOT NULL,
ID_CLIENT NUMBER(6),  
SUMA_CREDIT NUMBER(12),
PERIOADA_RAMBURSARE NUMBER(3),
DATA_ACORDARE DATE
);
ALTER TABLE CREDITE
  ADD CONSTRAINT CREDITE_FK FOREIGN KEY (ID_CLIENT) REFERENCES CLIENTI(ID_CLIENT);
ALTER TABLE CREDITE
  ADD CONSTRAINT P_KEY_CR PRIMARY KEY (ID_CREDIT);
ALTER TABLE CREDITE
  ADD CONSTRAINT CRED_MIN CHECK (SUMA_CREDIT > 0);
ALTER TABLE CREDITE
  ADD CONSTRAINT DATA_AC_NN CHECK("DATA_ACORDARE" IS NOT NULL);
ALTER TABLE CREDITE
  ADD CONSTRAINT PER_RAM CHECK ("PERIOADA_RAMBURSARE" IS NOT NULL);


prompt
prompt Creating table CARDURI
prompt ======================
prompt

CREATE TABLE CARDURI(
ID_CARD NUMBER(8) NOT NULL, 
ID_CLIENT NUMBER(6),
NUMAR_CARD VARCHAR2(16), 
TIP_CARD   VARCHAR2(20),
DATA_EXPIRARE DATE                   
);
ALTER TABLE CARDURI
  ADD CONSTRAINT P_KEY_CA PRIMARY KEY(ID_CARD);
ALTER TABLE CARDURI
  ADD CONSTRAINT CARD_CLIENT_FK FOREIGN KEY (ID_CLIENT) REFERENCES CLIENTI(ID_CLIENT) ON DELETE CASCADE;
ALTER TABLE CARDURI
  ADD CONSTRAINT NR_CARD_NN CHECK("NUMAR_CARD" IS NOT NULL);
ALTER TABLE CARDURI
  ADD CONSTRAINT DATA_EX_NN CHECK("DATA_EXPIRARE" IS NOT NULL);

prompt Done
spool off
set define on

prompt Disabling foreign key constraints
ALTER TABLE CLIENTI DISABLE CONSTRAINT TARA_CL_FK;
ALTER TABLE SUCURSALE DISABLE CONSTRAINT SUC_T_FK;
ALTER TABLE ANGAJATI DISABLE CONSTRAINT ANGJ_S_FK;
ALTER TABLE CONTURI DISABLE CONSTRAINT CONT_CL_FK;
ALTER TABLE TRANZACTII DISABLE CONSTRAINT CONT_T_FK;
ALTER TABLE TRANZACTII DISABLE CONSTRAINT TRANZ_A_FK;
ALTER TABLE CREDITE DISABLE CONSTRAINT CREDITE_FK;
ALTER TABLE CARDURI DISABLE CONSTRAINT CARD_CLIENT_FK;

prompt Truncating tables

TRUNCATE TABLE TARI;
TRUNCATE TABLE SUCURSALE;
TRUNCATE TABLE CLIENTI;
TRUNCATE TABLE ANGAJATI;
TRUNCATE TABLE TRANZACTII;
TRUNCATE TABLE CREDITE;
TRUNCATE TABLE CONTURI;
TRUNCATE TABLE CARDURI;

